{
  "name": "@slimio/timer",
  "version": "1.0.2",
  "description": "SlimIO - NodeJS Driftless &amp; Safe Timers",
  "main": "index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && nyc --reporter=lcov node test/test.js",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "codecov",
    "report": "nyc report --reporter=html"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Timer.git"
  },
  "keywords": [
    "SlimIO",
    "Node.js",
    "Timer",
    "No-drift",
    "Driftless"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Timer/issues"
  },
  "homepage": "https://github.com/SlimIO/Timer#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^8.3.4",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^4.1.0",
    "@slimio/psp": "^0.11.1",
    "codecov": "^3.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "husky": "^4.2.5",
    "japa": "^3.0.1",
    "jsdoc": "^3.6.4",
    "nyc": "^15.0.1",
    "pkg-ok": "^2.3.1"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  }
}
