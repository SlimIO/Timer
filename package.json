{
  "name": "@slimio/timer",
  "version": "1.0.1",
  "description": "SlimIO - NodeJS Driftless &amp; Safe Timers",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "node test/test.js",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env npm test && eslint index.js",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Timer.git"
  },
  "keywords": [
    "SlimIO",
    "monitoring"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Timer/issues"
  },
  "homepage": "https://github.com/SlimIO/Timer#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^2.0.3",
    "@types/node": "^10.12.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.7.0",
    "husky": "^1.1.2",
    "japa": "^2.0.6",
    "jsdoc": "^3.5.5",
    "nyc": "^13.1.0",
    "pkg-ok": "^2.3.1"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  }
}
